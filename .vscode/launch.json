{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Pico 2 (OpenOCD)",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/out/build/vgc/game.elf",
            "device": "RP2350",
            "interface": "swd",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/rp2350.cfg"
            ],
            "svdFile": "${workspaceFolder}/../pico-sdk/src/rp2350/hardware_regs/RP2350.svd",
            "gdbPath": "gdb-multiarch",
            "runToEntryPoint": "main",
            "overrideLaunchCommands": [
                "monitor reset init",
                "load \"${workspaceFolder}/out/build/vgc/game.elf\""
            ],
            "openOCDLaunchCommands": [
                "adapter speed 5000"
            ],
            "preLaunchTask": "Build Pico 2 (out/build/vgc)",
        },
    ]
}